// def
//   qsort( [] )    =  []
//   qsort( p:xs )  =  qsort( filter((<p), xs) ) + [p] + qsort( filter((>=p), xs) )

import lists.partition

def
	qsort( [] )    =  []
	qsort( x:xs )  =
		val (ys, zs) = partition( (<x), xs )
		qsort( ys ) + (x : qsort( zs ))

println( qsort([4, 2, 1, 3, 0, 2]) )
println( qsort(["Bob", "Alice", "Barry", "Zoe", "Charlotte", "Fred"]) )
