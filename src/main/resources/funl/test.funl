//Miller-Rabin_primality_test

import util.rnd

def isProbablyPrimeMillerRabin( n, k ) =
	n_1 = n - 1
	d = n_1
	s = 0
	
	while 2|d
		s++
		d /= 2

	for _ <- 1..k
		a = rnd( 2, n )
		x = a^d%n

		if x == 1 or x == n - 1 then continue

		for r <- 1..s - 1
			x = x^2%n
			
			if x == 1 then return false

			if x == n_1 then break
		else
			return false

	true

for i <- 3..100
	if isProbablyPrimeMillerRabin( i, 5 )
		println( i )