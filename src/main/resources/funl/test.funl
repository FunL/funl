def fold( f, z, it::Iterator ) =
	def iterfold( z ) =
		if it.hasNext()
			iterfold( f(z, it.next()) )
		else
			z

	iterfold( z )

println( fold((a, b) -> a + b, 0, k | k <- 1..200) )

