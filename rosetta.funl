//
// Ackermann function
//
def
	ack( 0, n ) = n + 1
	ack( m, 0 ) = ack( m - 1, 1 )
	ack( m, n ) = ack( m - 1, ack(m, n - 1) )

//
// Greatest element of a list
//
def
	foldl( f, z, [] )				= z
	foldl( f, z, x:xs )			= foldl( f, f(z, x), xs )

	foldl1( f, x:xs )				= foldl( f, x, xs )
	foldl1( _, [] )					= error( "foldl1: empty list" )

	max( a, b )							= if a <= b then b else a

	maximum( xs )						= foldl1( max, xs )